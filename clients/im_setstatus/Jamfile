SubDir TOP clients im_setstatus ;

SubDirHdrs [ FDirName $(TOP) ] ;

Application im_setstatus :
	im_setstatus.cpp
	: be libim.so
;

Depends im_setstatus : libim.so ;

InstallBin $(IMKIT_COMMON_BIN_DIRECTORY) : im_setstatus ;

Package imkit :
	im_setstatus :
	[ FSplitPath $(IMKIT_COMMON_BIN_DIRECTORY) ]
;
