SubDir HAIKU_TOP 3rdparty IMKit clients im_setstatus ;

SubDirC++Flags $(IMKIT_DEFINES) ;

SubDirSysHdrs [ FDirName $(IMKIT_TOP) ] ;

Application im_setstatus :
	im_setstatus.cpp
	: be libim.so
;

Depends im_setstatus : libim.so ;

AddFilesToHaikuImage common bin : im_setstatus ;

Package imkit-cvs :
	im_setstatus :
	boot common bin
;
