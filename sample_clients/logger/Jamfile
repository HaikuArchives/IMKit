SubDir IMKIT_TOP sample_clients logger ;

SubDirHdrs [ FDirName $(IMKIT_TOP) ] ;

Executable im_logger :
	logger.cpp
	: be libim.so
	: logger.rsrc
;

Depends im_logger : libim.so ;

InstallBin $(IMKIT_COMMON_BIN_DIRECTORY) : im_logger ;
