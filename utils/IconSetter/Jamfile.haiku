SubDir HAIKU_TOP 3rdparty IMKit utils IconSetter ;

SubDirC++Flags $(IMKIT_DEFINES) ;

SubDirSysHdrs [ FDirName $(IMKIT_TOP) ] ;

Application IconSetter :
	main.cpp
	: be translation libim.so
;

Depends IconSetter : libim.so ;

AddFilesToHaikuImage apps im_kit : IconSetter ;

Package imkit-cvs :
	IconSetter :
	boot apps im_kit
;
